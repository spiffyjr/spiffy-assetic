{
    "name": "spiffy/spiffy-assetic",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "description": "Spiffy\\AsseticPackage is a package for SpiffyFramework that helps you manage your assets.",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4",
        "kriswallsmith/assetic": "~1.1",
        "spiffy/spiffy-event": "~1.0",
        "symfony/finder": "~2.2",
        "twig/twig": "~1.15"
    },
    "require-dev": {
        "ptachoire/cssembed": "~1.0",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "suggest": {
        "mrclay/minify": "required to use the jsmin filter.",
        "natxet/cssmin": "required to use the cssmin filter.",
        "ptachoire/cssembed": "required to use the cssembed filter."
    },
    "autoload": {
        "psr-4": {
            "Spiffy\\Assetic\\": [
                "src/",
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    }
}
