{
    "name": "spiffy/spiffy-assetic-package",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "description": "Spiffy\\AsseticPackage is a package for SpiffyFramework that helps you manage your assets.",
    "require": {
        "php": ">=5.4",
        "kriswallsmith/assetic": "~1.1",
        "symfony/finder": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "suggest": {
        "natxet/cssmin": "Package required to use the included cssmin filter."
    },
    "autoload": {
        "psr-4": {
            "Spiffy\\AsseticPackage\\": [
                "src/",
                "test/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    }
}
